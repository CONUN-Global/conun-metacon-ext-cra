{
  "name": "Metacon",
  "description": "Metacon extension",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/metacon-icon-16.png",
    "24": "icons/metacon-icon-24.png",
    "32": "icons/metacon-icon-32.png",
    "48": "icons/metacon-icon-48.png",
    "96": "icons/metacon-icon-96.png",
    "128": "icons/metacon-icon-128.png",
    "256": "icons/metacon-icon-256.png"
  },
  "action": {
    "default_icon": "conun-logo.svg",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["http://localhost:3333/*"]
  },
  "permissions": ["activeTab", "storage", "tabs"]
}
